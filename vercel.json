{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run vercel-build",
        "outputDirectory": "web-build",
        "installCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.png",
      "dest": "/assets/icon.png"
    },
    {
      "src": "/assets/favicon.png",
      "dest": "/assets/icon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 